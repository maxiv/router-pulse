{
    "name": "Router Pulse",
    "description": "System to log router state and provider availability",
    "keywords": ["framework", "laravel", "lumen", "router", "mikrotik"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.1.*",
        "doctrine/dbal": "v2.5.3",
        "vlucas/phpdotenv": "~1.0",
        "illuminate/mail": "5.1.28"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
